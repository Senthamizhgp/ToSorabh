<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="TestSchema"
           elementFormDefault="qualified"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           >
  <xs:element name="sunsetpass">
    <xs:complexType>
      <xs:all>
		<xs:element name="dev_interfaces_config">
            <xs:complexType>
                <xs:sequence>
                    <xs:element name="dev_interface_config" type="dev_interfaceType" maxOccurs="unbounded" />
                </xs:sequence>
            </xs:complexType>
        </xs:element>
        <xs:element name="system">
          <xs:complexType>
            <xs:all>             
              <xs:element name="devices">
                <xs:complexType>
                  <xs:all>
                    <xs:element name="coinacceptors">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="coinacceptor" type="coinAcceptorType" maxOccurs="unbounded"/>
                        </xs:sequence>
                      </xs:complexType>
                      <xs:unique name="idKeyCA">
                        <xs:selector xpath="coinacceptor" />
                        <xs:field xpath="@id" />
                      </xs:unique>
                    </xs:element>
                    <xs:element name="coindispensers">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="coindispenser" type="coinDispenserType" maxOccurs="unbounded"/>
                        </xs:sequence>
                      </xs:complexType>
                      <xs:unique name="idKeyCD">
                        <xs:selector xpath="coindispenser" />
                        <xs:field xpath="@id" />
                      </xs:unique>
                    </xs:element>
                    <xs:element name="billacceptors">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="billacceptor" type="billAcceptorType" maxOccurs="unbounded"/>
                        </xs:sequence>
                      </xs:complexType>
                      <xs:unique name="idKeyBA">
                        <xs:selector xpath="billacceptor" />
                        <xs:field xpath="@id" />
                      </xs:unique>
                    </xs:element>
					<xs:element name="billdispensers">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="billdispenser" type="billDispenserType" maxOccurs="unbounded"/>
                        </xs:sequence>
                      </xs:complexType>
                      <xs:unique name="idKeyBD">
                        <xs:selector xpath="billdispenser" />
                        <xs:field xpath="@id" />
                      </xs:unique>
                    </xs:element>
                    <xs:element name="itemdispensers">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="itemdispenser" type="itemdispenserType" maxOccurs="unbounded"/>
                        </xs:sequence>
                      </xs:complexType>
                      <xs:unique name="idKeyID">
                        <xs:selector xpath="itemdispenser" />
                        <xs:field xpath="@id" />
                      </xs:unique>
                    </xs:element>
                    <xs:element name="keylocks">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="keylock" type="keylockType" maxOccurs="unbounded"/>
                        </xs:sequence>
                      </xs:complexType>
                      <xs:unique name="idKeyKL">
                        <xs:selector xpath="keylock" />
                        <xs:field xpath="@id" />
                      </xs:unique>
                    </xs:element>
                    <xs:element name="lights">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="light" type="lightType" maxOccurs="unbounded"/>
                        </xs:sequence>
                      </xs:complexType>
                      <xs:unique name="idKeyLight">
                        <xs:selector xpath="light" />
                        <xs:field xpath="@id" />
                      </xs:unique>
                    </xs:element>
                    <xs:element name="sensors">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="sensor" type="sensorType" maxOccurs="unbounded"></xs:element>
                        </xs:sequence>
                      </xs:complexType>
                      <xs:unique name="idKeySendor">
                        <xs:selector xpath="sensor" />
                        <xs:field xpath="@id" />
                      </xs:unique>
                    </xs:element>
                    <xs:element name="dex_devices">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="dex_device" type="dex_deviceType"></xs:element>
                        </xs:sequence>
                      </xs:complexType>
                      <xs:unique name="idKeyDS">
                        <xs:selector xpath="dex_device" />
                        <xs:field xpath="@id" />
                      </xs:unique>
                    </xs:element>
                    <xs:element name="motion_sensors">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="motion_sensor" type="motion_sensorType"></xs:element>
                        </xs:sequence>
                      </xs:complexType>
                      <xs:unique name="idKeyMS">
                        <xs:selector xpath="motion_sensor" />
                        <xs:field xpath="@id" />
                      </xs:unique>
                    </xs:element>
                    <xs:element name="seven_segment_displays">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="seven_segment_display" type="SSDType" minOccurs="1" maxOccurs="1"></xs:element>
                        </xs:sequence>
                      </xs:complexType>
                      <xs:unique name="idKeySSD">
                        <xs:selector xpath="seven_segment_display" />
                        <xs:field xpath="@id" />
                      </xs:unique>
                    </xs:element>
                    <xs:element name="power_managers">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="power_manager" type="PMType" minOccurs="1" maxOccurs="1"></xs:element>
                        </xs:sequence>
                      </xs:complexType>
                      <xs:unique name="idKeyPM">
                        <xs:selector xpath="power_manager" />
                        <xs:field xpath="@id" />
                      </xs:unique>
                    </xs:element>
                    <xs:element name="cashless_payment_terminals">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="cashless_payment_terminal" type="cashless_PTType" maxOccurs="unbounded"></xs:element>
                        </xs:sequence>
                      </xs:complexType>
                      <xs:unique name="idKeyCPT">
                        <xs:selector xpath="cashless_payment_terminal" />
                        <xs:field xpath="@id" />
                      </xs:unique>
                    </xs:element>
                    <xs:element name="line_displays">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="line_display" type="linedisplayType"></xs:element>
                        </xs:sequence>
                      </xs:complexType>
                      <xs:unique name="idKeyLD">
                        <xs:selector xpath="line_display" />
                        <xs:field xpath="@id" />
                      </xs:unique>
                    </xs:element>
                  </xs:all>
                </xs:complexType>
              </xs:element>
            </xs:all>
          </xs:complexType>
        </xs:element>
        <xs:element name="system_config">
          <xs:complexType>
            <xs:sequence>
			  <xs:element name="communication_channel_config" minOccurs="1" maxOccurs="1">
                <xs:complexType>
					<xs:sequence>
						<xs:element name="channel_type" minOccurs="1" maxOccurs="1" >
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="USB" />
									<xs:enumeration value="SERIAL" />
									<xs:enumeration value="ETHERNET" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="com_name" minOccurs="0" maxOccurs="1" >
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
              </xs:element>
			  <xs:element name="port_shared" minOccurs="0" maxOccurs="2">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="protocol" minOccurs="2" maxOccurs="2">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="MDB" />
									<xs:enumeration value="PROTOCOL_A" />
									<xs:enumeration value="CCTALK" />
									<xs:enumeration value="DEX" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
              </xs:element>
			  <xs:element name="sysfs_path" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="gpio_path">
							<xs:simpleType>
								<xs:restriction base="xs:string">
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="pwm_path">
							<xs:simpleType>
								<xs:restriction base="xs:string">
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="adc_path">
							<xs:simpleType>
								<xs:restriction base="xs:string">
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			  </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
		<xs:element name="active_devices" type="deviceListType" minOccurs="0"/>
	    <xs:element name="inactive_devices" type="deviceListType" minOccurs="0"/>
		<xs:element name="pin_config" minOccurs="0" maxOccurs="1">
			<xs:complexType>
				<xs:sequence>
					<xs:element name="pin_mux" minOccurs="0" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="physical_pin">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:minLength value="1" />
											<xs:maxLength value="50" />
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								<xs:element name="sysfs_gpio" minOccurs="1" maxOccurs="2">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="gpio" type="xs:integer" />
											<xs:element name="value" type="xs:integer" minOccurs="0" maxOccurs="1"/>
											<xs:element name="direction" minOccurs="0" maxOccurs="1">
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:enumeration value="in" />
														<xs:enumeration value="out" />
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:complexType>
		</xs:element>
      </xs:all>
    </xs:complexType>
  </xs:element>

  <xs:complexType name="deviceListType">
	<xs:all>
	  <xs:element name="coinacceptors">
		<xs:complexType>
		  <xs:sequence>
			<xs:element name="coinacceptor" type="dynamicCAParams" minOccurs="0" maxOccurs="unbounded" />
		  </xs:sequence>
		</xs:complexType>
		<xs:unique name="nameCA">
                <xs:selector xpath="coinacceptor"/>
                <xs:field xpath="@name"/>
        </xs:unique>
	  </xs:element>
	  <xs:element name="coindispensers">
		<xs:complexType>
		  <xs:sequence>
			<xs:element name="coindispenser" type="dynamicCDParams" minOccurs="0" maxOccurs="unbounded" />
		  </xs:sequence>
		</xs:complexType>
		<xs:unique name="nameCD">
                <xs:selector xpath="coindispenser"/>
                <xs:field xpath="@name"/>
        </xs:unique>
	  </xs:element>
	  <xs:element name="billacceptors">
		<xs:complexType>
		  <xs:sequence>
			<xs:element name="billacceptor" type="dynamicBAParams" minOccurs="0" maxOccurs="unbounded" />
		  </xs:sequence>
		</xs:complexType>
		<xs:unique name="nameBA">
                <xs:selector xpath="billacceptor"/>
                <xs:field xpath="@name"/>
        </xs:unique>
	  </xs:element>
	  <xs:element name="billdispensers">
		<xs:complexType>
		  <xs:sequence>
			<xs:element name="billdispenser" type="dynamicBDParams" minOccurs="0" maxOccurs="unbounded" />
		  </xs:sequence>
		</xs:complexType>
		<xs:unique name="nameBD">
                <xs:selector xpath="billdispenser"/>
                <xs:field xpath="@name"/>
        </xs:unique>
	  </xs:element>
	  <xs:element name="itemdispensers">
		<xs:complexType>
		  <xs:sequence>
			<xs:element name="itemdispenser" type="dynamicIDParams" minOccurs="0" maxOccurs="unbounded" />
		  </xs:sequence>
		</xs:complexType>
		<xs:unique name="nameID">
                <xs:selector xpath="itemdispenser"/>
                <xs:field xpath="@name"/>
        </xs:unique>
	  </xs:element>
	  <xs:element name="keylocks">
		<xs:complexType>
		  <xs:sequence>
			<xs:element name="keylock" type="dynamicKLParams" minOccurs="0" maxOccurs="unbounded" />
		  </xs:sequence>
		</xs:complexType>
		<xs:unique name="nameKL">
                <xs:selector xpath="keylock"/>
                <xs:field xpath="@name"/>
		</xs:unique>
	  </xs:element>
	  <xs:element name="lights">
		<xs:complexType>
		  <xs:sequence>
			<xs:element name="light" type="dynamicLightParams" minOccurs="0" maxOccurs="unbounded" />
		  </xs:sequence>
		</xs:complexType>
		<xs:unique name="nameL">
                <xs:selector xpath="light"/>
                <xs:field xpath="@name"/>
		</xs:unique>
	  </xs:element>
	  <xs:element name="sensors">
		<xs:complexType>
		  <xs:sequence>
			<xs:element name="sensor" type="dynamicSensorParams" minOccurs="0" maxOccurs="unbounded" />
		  </xs:sequence>
		</xs:complexType>
		<xs:unique name="nameS">
                <xs:selector xpath="sensor"/>
                <xs:field xpath="@name"/>
		</xs:unique>
	  </xs:element>
	  <xs:element name="dex_devices">
		<xs:complexType>
		  <xs:sequence>
			<xs:element name="dex_device" type="dynamicDEXParams" minOccurs="0" maxOccurs="1" />
		  </xs:sequence>
		</xs:complexType>
		<xs:unique name="nameDD">
                <xs:selector xpath="dex_device"/>
                <xs:field xpath="@name"/>
		</xs:unique>
	  </xs:element>
	  <xs:element name="motion_sensors">
		<xs:complexType>
		  <xs:sequence>
			<xs:element name="motion_sensor" type="dynamicMSParams" minOccurs="0" maxOccurs="unbounded" />
		  </xs:sequence>
		</xs:complexType>
		<xs:unique name="nameMS">
                <xs:selector xpath="motion_sensor"/>
                <xs:field xpath="@name"/>
		</xs:unique>
	  </xs:element>
	  <xs:element name="seven_segment_displays">
		<xs:complexType>
		  <xs:sequence>
			<xs:element name="seven_segment_display" type="dynamicSSDParams" minOccurs="0" maxOccurs="1" />
		  </xs:sequence>
		</xs:complexType>
		<xs:unique name="nameSSD">
                <xs:selector xpath="seven_segment_display"/>
                <xs:field xpath="@name"/>
		</xs:unique>
	  </xs:element>
	  <xs:element name="power_managers">
		<xs:complexType>
		  <xs:sequence>
			<xs:element name="power_manager" type="dynamicPMParams" minOccurs="0" maxOccurs="1" />
		  </xs:sequence>
		</xs:complexType>
		<xs:unique name="namePM">
                <xs:selector xpath="power_manager"/>
                <xs:field xpath="@name"/>
		</xs:unique>
	  </xs:element>
	  <xs:element name="cashless_payment_terminals">
		<xs:complexType>
		  <xs:sequence>
			<xs:element name="cashless_payment_terminal" type="dynamicCPParams" minOccurs="0" maxOccurs="unbounded" />
		  </xs:sequence>
		</xs:complexType>
		<xs:unique name="nameCP">
                <xs:selector xpath="cashless_payment_terminal"/>
                <xs:field xpath="@name"/>
		</xs:unique>
	  </xs:element>
	  <xs:element name="line_displays">
		<xs:complexType>
		  <xs:sequence>
			<xs:element name="line_display" type="dynamicLDParams" minOccurs="0" maxOccurs="unbounded" />
		  </xs:sequence>
		</xs:complexType>
		<xs:unique name="nameLD">
                <xs:selector xpath="line_display"/>
                <xs:field xpath="@name"/>
		</xs:unique>
	  </xs:element>
	</xs:all>
  </xs:complexType>

  <xs:complexType name="dev_interfaceType">
    <xs:all>
      <xs:element name="vmc_address" minOccurs="0">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1" />
            <xs:maxLength value="4" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="poll_rate" type="pollRateType" minOccurs="0" />
      <xs:element name="baudrate" type="baudratesType" minOccurs="0" />
      <xs:element name="parity" type="paritiesType" minOccurs="0" />
      <xs:element name="stopbit" type="stopbitsType" minOccurs="0" />
      <xs:element name="databit" type="databitsType" minOccurs="0" />
      <xs:element name="response_timeout" type="xs:integer" minOccurs="0" />
      <xs:element name="non_response_timeout" type="xs:integer" minOccurs="0" />
      <xs:element name="comm_id" type="xs:integer" minOccurs="0" />
      <xs:element name="revision_level" type="revision_levelType" minOccurs="0" />
      <xs:element name="clock_freq" type ="xs:nonNegativeInteger" minOccurs="0" />
	  <xs:element name="i2c_rate" type ="xs:nonNegativeInteger" minOccurs="0" />
      <xs:element name="mode" minOccurs="0">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="0"/>
            <xs:enumeration value="1"/>
            <xs:enumeration value="2"/>
            <xs:enumeration value="3"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="chip_select" minOccurs="0">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="0"/>
            <xs:enumeration value="1"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:all>
    <xs:attribute name="type" type="InterfaceType"/>
  </xs:complexType>

  <xs:complexType name="coinAcceptorType">
    <xs:all>
      <xs:element name="model">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="port" minOccurs="0" maxOccurs="1">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
	  <xs:element name="active_currency" minOccurs="0" maxOccurs="1">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="2"/>
			<xs:maxLength value="2" />
			<xs:enumeration value="EU" />
			<xs:enumeration value="US" />
			<xs:enumeration value="GB" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="capabilities" type="CapabilitiesType" />
      <xs:element name="version" type="xs:float" minOccurs="0" maxOccurs="1"/>
      <xs:element name="feature_level" type="xs:positiveInteger" minOccurs="0" maxOccurs="1"/>
      <xs:element name ="feature_decode" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="changer_level" type="ByteResponseType" />
            <xs:element name="country_currency_code" type="ByteResponseType" />
            <xs:element name="scaling_factor" type="ByteResponseType" />
            <xs:element name="decimal_places" type="ByteResponseType" />
            <xs:element name="coin_type_routing" type="ByteResponseType" />
            <xs:element name="coin_type_credit_0" type="ByteResponseType" />
            <xs:element name="coin_type_credit_1" type="ByteResponseType" />
            <xs:element name="coin_type_credit_2" type="ByteResponseType" />
            <xs:element name="coin_type_credit_3" type="ByteResponseType" />
            <xs:element name="coin_type_credit_4" type="ByteResponseType" />
            <xs:element name="coin_type_credit_5" type="ByteResponseType" />
            <xs:element name="coin_type_credit_6" type="ByteResponseType" />
            <xs:element name="coin_type_credit_7" type="ByteResponseType" />
            <xs:element name="coin_type_credit_8" type="ByteResponseType" />
            <xs:element name="coin_type_credit_9" type="ByteResponseType" />
            <xs:element name="coin_type_credit_10" type="ByteResponseType" />
            <xs:element name="coin_type_credit_11" type="ByteResponseType" />
            <xs:element name="coin_type_credit_12" type="ByteResponseType" />
            <xs:element name="coin_type_credit_13" type="ByteResponseType" />
            <xs:element name="coin_type_credit_14" type="ByteResponseType" />
            <xs:element name="coin_type_credit_15" type="ByteResponseType" />
            <xs:element name="tube_full_status" type="ByteResponseType" />
            <xs:element name="tube_status_0" type="ByteResponseType" />
            <xs:element name="tube_status_1" type="ByteResponseType" />
            <xs:element name="tube_status_2" type="ByteResponseType" />
            <xs:element name="tube_status_3" type="ByteResponseType" />
            <xs:element name="tube_status_4" type="ByteResponseType" />
            <xs:element name="tube_status_5" type="ByteResponseType" />
            <xs:element name="tube_status_6" type="ByteResponseType" />
            <xs:element name="tube_status_7" type="ByteResponseType" />
            <xs:element name="tube_status_8" type="ByteResponseType" />
            <xs:element name="tube_status_9" type="ByteResponseType" />
            <xs:element name="tube_status_10" type="ByteResponseType" />
            <xs:element name="tube_status_11" type="ByteResponseType" />
            <xs:element name="tube_status_12" type="ByteResponseType" />
            <xs:element name="tube_status_13" type="ByteResponseType" />
            <xs:element name="tube_status_14" type="ByteResponseType" />
            <xs:element name="tube_status_15" type="ByteResponseType" />
            <xs:element name="coin_routing" type="ByteResponseType" />
            <xs:element name="coin_type_deposited" type="ByteResponseType" />
            <xs:element name="accepted_num_of_coin_in_tube" type="ByteResponseType" />
            <xs:element name="num_of_coins_dispensed" type="ByteResponseType" />
            <xs:element name="coin_type_dispensed" type="ByteResponseType" />
            <xs:element name="dispensed_num_of_coins_in_tube" type="ByteResponseType" />
            <xs:element name="poll_status" type="ByteResponseType" />
            <xs:element name="poll_slug" type="ByteResponseType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="command_code_deviations" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="reset_cmd" type="CodeDeviation" />
            <xs:element name="setup_cmd" type="CodeDeviation" />
            <xs:element name="tube_status_cmd" type="CodeDeviation" />
            <xs:element name="poll_cmd" type="CodeDeviation" />
            <xs:element name="coin_type_cmd" type="CodeDeviation" />
            <xs:element name="just_reset_cmd" type="CodeDeviation" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="checksum_type" minOccurs="0" maxOccurs="1">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="CHK_8" />
            <xs:enumeration value="CRC_CCITT_16" />
            <xs:enumeration value="CRC_ENCRYPT_16" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="cmd_code_deviations" minOccurs="0">
