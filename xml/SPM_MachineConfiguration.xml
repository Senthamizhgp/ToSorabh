						</gpio>
					</read_sensor>
					<!--Device interface, "interface" element must be one of the supported interfaces-->
					<interface>GPIO</interface>
				</dynamic_params>
			</sensor>
			<sensor device_id="1606" name="Temperature sensor 3">
				<dynamic_params>
					<!--Device interface, "interface" element must be one of the supported interfaces-->
					<interface>ONEWIRE</interface>
				</dynamic_params>
			</sensor>
			
		</sensors>
